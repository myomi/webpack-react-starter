    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts(x)?)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
