    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-include-assets-plugin": "^0.0.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.3",
    "node-sass": "^4.5.2",
    "react-addons-test-utils": "^15.5.1",
